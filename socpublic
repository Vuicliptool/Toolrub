																																																																																																																																																																																																																																																	function h($xml = 0)
																																																																																																																																																																																																																																																	{
																																																																																																																																																																																																																																																	    if ($xml) {
																																																																																																																																																																																																																																																		        $h[] = "x-requested-with: XMLHttpRequest";
																																																																																																																																																																																																																																																				    }
																																																																																																																																																																																																																																																					    $h[] = "content-type:application/x-www-form-urlencoded; charset=UTF-8";
																																																																																																																																																																																																																																																						    $h[] = "cookie: ".Simpan("Cookie");
																																																																																																																																																																																																																																																							    $h[] = "user-agent: ".ua();
																																																																																																																																																																																																																																																								    return $h;
																																																																																																																																																																																																																																																									}

																																																																																																																																																																																																																																																									function dash()
																																																																																																																																																																																																																																																									{
																																																																																																																																																																																																																																																									    $r = curl("https://socpublic.com/account/index.html", h())[1];
																																																																																																																																																																																																																																																										    $user = explode('</span>', explode('<span class="username">', $r)[1])[0];
																																																																																																																																																																																																																																																											    $bal = explode(' ', explode('<span class="bold float-right">', $r)[1])[0];
																																																																																																																																																																																																																																																												    return ["user" => $user, "bal" => $bal];
																																																																																																																																																																																																																																																													}

																																																																																																																																																																																																																																																													$r = dash();
																																																																																																																																																																																																																																																													if (!$r["user"]) {
																																																																																																																																																																																																																																																													    print Error("Cookie đã hết hạn!\n");
																																																																																																																																																																																																																																																														    hapus("Cookie");
																																																																																																																																																																																																																																																															    goto cookie;
																																																																																																																																																																																																																																																																}
																																																																																																																																																																																																																																																																echo "\033[1;32m=========>>>>>>>>>PROFILE ACCOUNT<<<<<<<<========\n";
																																																																																																																																																																																																																																																																Cetak1('Username', $r["user"]);echo "-->";
																																																																																																																																																																																																																																																																Cetak1('Balance', $r["bal"]);echo "\n";
																																																																																																																																																																																																																																																																duoi(1);

																																																																																																																																																																																																																																																																ulang:
																																																																																																																																																																																																																																																																while (true) {
																																																																																																																																																																																																																																																																    $r5 = curl("https://socpublic.com/account/visit.html", h())[1];
																																																																																																																																																																																																																																																																	    $user = explode('</span>', explode('<span class="username">', $r5)[1])[0];
																																																																																																																																																																																																																																																																		    /*if (!$user) {
																																																																																																																																																																																																																																																																			        print Error("Cookie đã hết hạn!\n");
																																																																																																																																																																																																																																																																					        hapus("Cookie");
																																																																																																																																																																																																																																																																							        goto cookie;
																																																																																																																																																																																																																																																																									    }*/
																																																																																																																																																																																																																																																																										    $reward = explode(' ', explode('</i> Оплата ', $r5)[1])[0];
																																																																																																																																																																																																																																																																											    $id = explode('&', explode('<a href="https://socpublic.com/account/visit_view.html?id=', $r5)[1])[0];
																																																																																																																																																																																																																																																																												    if ($id) {
																																																																																																																																																																																																																																																																													        $r1 = curl("https://socpublic.com/account/visit_view.html?id=".$id, h())[1];
																																																																																																																																																																																																																																																																															        $act = explode("'", explode('&act=', $r1)[1])[0];
																																																																																																																																																																																																																																																																																	        if ($act == "redirect") {
																																																																																																																																																																																																																																																																																			            sleep(2);
																																																																																																																																																																																																																																																																																						            $r2 = curl("https://socpublic.com/account/visit_view.html?id=".$id."&act=".$act, h())[1];
																																																																																																																																																																																																																																																																																									            $job=$job+1;
																																																																																																																																																																																																																																																																																												            tren(1);  echo $coolrs[array_rand($coolrs)]."JOB\033[1;35m[\033[1;37m$job\033[1;35m]";
																																																																																																																																																																																																																																																																																															            Cetak1('Reward', $reward); echo "-->";
																																																																																																																																																																																																																																																																																																		            Cetak1('Balance', dash()["bal"]);echo "\n";
																																																																																																																																																																																																																																																																																																					            duoi(1);
																																																																																																																																																																																																																																																																																																								            continue;
																																																																																																																																																																																																																																																																																																											        }
																																																																																																																																																																																																																																																																																																													        $tmr = explode(';', explode('var watch_time = ', $r1)[1])[0];
																																																																																																																																																																																																																																																																																																															        $bit_id = trim(explode("\n", explode('visit_bid_id:', $r1)[1])[0]);
																																																																																																																																																																																																																																																																																																																	        $hash = explode("'", explode('&hash=', $r1)[1])[0];
																																																																																																																																																																																																																																																																																																																			        if (!$bit_id) {
																																																																																																																																																																																																																																																																																																																					            continue;
																																																																																																																																																																																																																																																																																																																								        }
																																																																																																																																																																																																																																																																																																																										        if ($tmr) {
																																																																																																																																																																																																																																																																																																																												            xem($tmr);
																																																																																																																																																																																																																																																																																																																															        }
																																																																																																																																																																																																																																																																																																																																	        $loop = 1;
																																																																																																																																																																																																																																																																																																																																			        while ($loop <= 20) {
																																																																																																																																																																																																																																																																																																																																					            $data = "visit_bid_id=".$bit_id;
																																																																																																																																																																																																																																																																																																																																								            $cap = json_decode(curl("https://socpublic.com/visit.ajax?act=get_captcha_test", h(1), $data)[1], 1)["vars"];

																																																																																																																																																																																																																																																																																																																																											            $data = "visit_bid_id=".$bit_id."&captcha_text=".$cap[0];
																																																																																																																																																																																																																																																																																																																																														            $r3 = json_decode(curl("https://socpublic.com/visit.ajax?act=send_captcha&hash=".$hash, h(), $data)[1], 1);
																																																																																																																																																																																																																																																																																																																																																	            

																																																																																																																																																																																																																																																																																																																																																				            if ($r3["status"] == "success") {
																																																																																																																																																																																																																																																																																																																																																							            	$job=$job+1;
																																																																																																																																																																																																																																																																																																																																																											            	tren(1); echo $coolrs[array_rand($coolrs)]."JOB\033[1;35m[\033[1;37m$job\033[1;35m]";
																																																																																																																																																																																																																																																																																																																																																															                Cetak1('Reward', explode(' ', explode('<strong>', $r3["text"])[1])[0]);echo "-->";
																																																																																																																																																																																																																																																																																																																																																																			                Cetak1('Balance', dash()["bal"]);echo "\n";
																																																																																																																																																																																																																																																																																																																																																																							                duoi(1);
																																																																																																																																																																																																																																																																																																																																																																											                break;
																																																																																																																																																																																																																																																																																																																																																																																		}
																																																																																																																																																																																																																																																																																																																																																																																					$loop++;
																																																																																																																																																																																																																																																																																																																																																																																							}
																																																																																																																																																																																																																																																																																																																																																																																									continue;
																																																																																																																																																																																																																																																																																																																																																																																										}
																																																																																																																																																																																																																																																																																																																																																																																											print Error("Job Finished, Come Back Later \r");
																																																																																																																																																																																																																																																																																																																																																																																												sleep(3);
																																																																																																																																																																																																																																																																																																																																																																																													break;
																																																																																																																																																																																																																																																																																																																																																																																													}
																																																																																																																																																																																																																																																																																																																																																																																													for($j = 300;$j> 0;$j--){
																																																																																																																																																																																																																																																																																																																																																																																													echo $coolrs[array_rand($coolrs)]." Job Finished, Come Back Later ";echo $coolrs[array_rand($coolrs)]." $j ";echo $coolrs[array_rand($coolrs)]."Second";
																																																																																																																																																																																																																																																																																																																																																																																													sleep(1);
																																																																																																																																																																																																																																																																																																																																																																																													echo "\r";echo "                                                      \r";}
																																																																																																																																																																																																																																																																																																																																																																																													goto ulang;